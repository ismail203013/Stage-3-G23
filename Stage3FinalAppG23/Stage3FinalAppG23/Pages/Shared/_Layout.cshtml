<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] | Rebates Center</title>
    <link rel="icon" href="~/Images/Icon-Elanco.png" type="image/png" />
    <link rel="stylesheet" href="~/styles/mobile.css" media="(max-width: 720px" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
</head>
<body>
    <div class="container">
        <header>
            @RenderBody()
        </header>
        <section>
            @RenderBody()
        </section>
        <footer>
            <div class="footer-container">
                <div id="affiliates">
                    <img src="~/Images/affiliates-1.png" width="50%" height="100" />
                    <img src="~/Images/affiliates-2.png" width="50%" height="100" />
                </div>
            </div>
        </footer>
    </div>
    <script src="~/js/main.js"></script>
</body>
</html>
